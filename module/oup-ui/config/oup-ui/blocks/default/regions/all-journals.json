{
  "evaluator": "and",
  "condition": [
    {
      "evaluator": "uri-match",
      "condition": "/projects/{projectId}/documents/{documentId}"
    },
    {
      "evaluator": "context-project"
    },
    {
      "evaluator": "context-document"
    },
    {
      "evaluator": "context-document-has-type",
      "condition": "my:alljournals"
    }
  ],
  "config": {
    "regions": {
      "center": {
        "type": "all-journals-list",
        "config": {
          "chrome": false,
          "loader": "gitana"
        }
      }
    }
  }
}

